<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/tvServicesName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="10dp"
        android:layout_marginStart="10dp"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteX="107dp"
        tools:layout_editor_absoluteY="94dp" />

    <TextView
        android:id="@+id/tvServicesType"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        app:layout_constraintEnd_toStartOf="@id/tvServicesCost"
        app:layout_constraintStart_toEndOf="@id/tvServicesName"
        app:layout_constraintTop_toTopOf="@id/tvServicesName"
        tools:layout_editor_absoluteX="221dp"
        tools:layout_editor_absoluteY="140dp" />

    <TextView
        android:id="@+id/tvServicesCost"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:text="TextView"
        app:layout_constraintTop_toTopOf="@id/tvServicesName"
        app:layout_constraintEnd_toEndOf="parent"
        tools:layout_editor_absoluteX="269dp"
        tools:layout_editor_absoluteY="222dp" />
</androidx.constraintlayout.widget.ConstraintLayout>